            document.getElementById("img4Disp").src = URL.createObjectURL(event.target.files[3])
            document.getElementById("img5Disp").src = URL.createObjectURL(event.target.files[4])
        }

        function removeImage1() {
            document.getElementById("img1Disp").src = null
        }
    </script>


